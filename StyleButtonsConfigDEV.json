      "Scripts": [
        {
          "Name": "ScheduleBold",
          "Comment": "Adds the word Schedule and manual formatting.",
          "Commands": [
            {
              "Name": "InsertText",
              "Text": "Schedule",
              "PreventDuplicates": true,
              "CommandRange": "StartOfParagraph"
            },
            {
              "Name": "ApplyFontFormatting",
              "SmallCaps": 1,
              "Bold": 1,
              "CommandRange": "Paragraphs"
            },
            {
              "Name": "ApplySausage",
              "Cheese": true
            }
          ]
        },
        {
          "Name": "H1Sched",
          "Comment": "Adds the word Schdule and style Heading 1 to the first paragraph.",
          "Commands": [
            {
              "Name": "InsertText",
              "Text": "Schedule",
              "PreventDuplicates": true,
              "CommandRange": "StartOfParagraph"
            },
            {
              "Name": "ApplyStyle",
              "StyleName": "Heading 1",
              "CommandRange": "FirstParagraph"
            }
          ]
        },
        {
          "Name": "SquareBrackets",
          "Comment": "Adds square brackets around the selection.",
          "Commands": [
            {
              "Name": "InsertText",
              "Text": "[",
              "CommandRange": "BeforeSelection"
            },
            {
              "Name": "InsertText",
              "Text": "]",
              "CommandRange": "AfterSelection"
            }
          ]
        },
        {
          "Name": "InsertField",
          "Comment": "Inserts a listnum field at the insertion point.",
          "Commands": [
            {
              "Name": "InsertField",
              "FieldText": "listnum /i",
              "CommandRange": "BeforeSelection"
            }
          ]
        }
